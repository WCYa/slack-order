{
    "display_information": {
        "name": "slack-order"
    },
    "features": {
        "bot_user": {
            "display_name": "slack-order",
            "always_online": false
        },
        "slash_commands": [
            {
                "command": "/order",
                "description": "開啟午餐訂單",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
